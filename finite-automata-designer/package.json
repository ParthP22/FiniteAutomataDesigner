{
  "name": "finite-automata-designer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "npm run build:canvas && npm run dev",
    "build": "next build",
    "build:canvas": "rollup -c rollup.dfa.config.js && rollup -c rollup.nfa.config.js",
