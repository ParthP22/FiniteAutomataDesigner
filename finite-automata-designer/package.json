    "build:nfa": "rollup -c rollup.nfa.config.js",
    "build:prod": "npm run build:canvas && npm run build:dfa && npm run build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0",
